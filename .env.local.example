# Copy this file to .env.local and fill in your keys

# Paystack (required for checkout)
# Get your keys at https://dashboard.paystack.com/#/settings/developers
PAYSTACK_SECRET_KEY=sk_test_...
NEXT_PUBLIC_PAYSTACK_PUBLIC_KEY=pk_test_...

# Currency for Paystack transactions (default: NGN)
# Supported: NGN, GHS, USD, ZAR
PAYSTACK_CURRENCY=NGN

# Your deployment URL (no trailing slash)
NEXT_PUBLIC_URL=http://localhost:3000

# Admin credentials (basic protection â€” use a proper auth provider in production)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=changeme
